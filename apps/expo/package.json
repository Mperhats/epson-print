{
  "name": "@epson-print/mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "EXPO_DEBUG=1 expo run:android",
    "ios": "EXPO_DEBUG=1 expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "type-check": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "prebuild:ios": "expo prebuild --platform ios",
    "prebuild:android": "expo prebuild --platform android",
    "clean": "rm -rf ios android && rm -rf ios android",
    "clean:ios": "rm -rf ios",
    "clean:android": "rm -rf android",
    "run:ios": "yarn prebuild:ios && yarn ios",
    "pod:install": "cd ios && pod install",
    "pod:update": "cd ios && pod update",
    "studio:android": "yarn prebuild:android && open -a /Applications/Android\\ Studio.app ./android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "14.0.2",
    "@nosh/backend-merchant-sdk": "workspace:*",
    "@react-navigation/native": "7.0.14",
    "expo": "^52.0.28",
    "expo-build-properties": "^0.13.2",
    "expo-constants": "~17.0.5",
    "expo-font": "13.0.3",
    "expo-linking": "7.0.5",
    "expo-router": "4.0.17",
    "expo-splash-screen": "0.29.21",
    "expo-status-bar": "2.0.1",
    "expo-system-ui": "4.0.7",
    "expo-web-browser": "14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-esc-pos-printer": "^4.3.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "4.0.0-nightly-20250121-4c66cb237",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "4.4.0",
    "react-native-view-shot": "^4.0.3",
    "react-native-web": "0.19.13"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@epson-print/typescript-config": "0.0.0",
    "@types/react": "18.3.12",
    "jest": "29.2.1",
    "jest-expo": "52.0.3",
    "react-test-renderer": "18.3.1",
    "typescript": "5.3.3"
  },
  "private": true,
  "expo-yarn-workspaces": {
    "symlinks": [
      "expo-constants"
    ]
  }
}
